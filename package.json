{
  "name": "Convert-CSV",
  "version": "1.0.14",
  "description": "CSV/XLS Converter Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "build -mw",
    "build-mac": "build --m -p never",
    "ship": "build --m -p always",
    "postversion": "git add . && git push && npm run ship",
    "ship-patch": "npm version patch --force -m \"version %s\"",
    "ship-minor": "npm version minor --force -m \"version %s\"",
    "ship-major": "npm version major --force -m \"version %s\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelkordek/convert-csv.git"
  },
  "keywords": [
    "electron",
    "csv",
    "xls",
    "json"
  ],
  "author": "Marcel Kordek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcelkordek/convert-csv/issues"
  },
  "homepage": "https://github.com/marcelkordek/convert-csv#readme",
  "devDependencies": {
    "electron": "^1.8.2",
    "electron-builder": "^20.2.0",
    "electron-builder-squirrel-windows": "^20.2.0"
  },
  "dependencies": {
    "alasql": "^0.4.5",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.14",
    "electron-updater": "^2.20.1"
  }
}
